{
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "bin": {
    "fool": "./dist/index.js"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "jest",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "fool-deploy",
  "author": "thomas-void0",
  "module": "dist/fool-deploy.esm.js",
  "version": "0.0.1-beta",
  "size-limit": [
    {
      "path": "dist/fool-deploy.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/fool-deploy.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/shelljs": "^0.8.11",
    "conventional-changelog-cli": "^2.2.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "size-limit": "^8.2.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "shelljs": "^0.8.5"
  }
}
